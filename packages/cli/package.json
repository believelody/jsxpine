{
	"name": "jsxpine",
	"version": "0.2.13",
	"description": "Add jsx components to your app.",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"author": {
		"name": "billybillydev",
		"url": "https://twitter.com/billybillydev"
	},
	"homepage": "https://github.com/believelody/jsxpine#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/believelody/jsxpine.git",
		"directory": "packages/cli"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"components",
		"ui",
		"tailwind",
		"radix-ui",
		"shadcn",
		"devdojo",
		"jsx",
		"pine-ui",
		"alpinejs"
	],
	"type": "module",
	"exports": "./dist/index.js",
	"bin": {
		"jsxpine": "dist/index.js"
	},
	"scripts": {
		"dev": "tsup --watch",
		"start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:3000/public node dist/index.js",
		"start": "node dist/index.js",
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf dist && rimraf components",
		"format:write": "prettier --write .",
		"format:check": "prettier --check .",
		"version": "changeset version",
		"release": "changeset publish",
		"pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
		"pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
		"pub:release": "pnpm build && pnpm publish --access public"
	},
	"dependencies": {
		"@alpinejs/collapse": "^3.13.10",
		"@alpinejs/focus": "^3.13.10",
		"@iconify-json/ri": "^1.1.20",
		"@iconify/utils": "^2.1.23",
		"alpinejs-manage": "^1.0.4",
		"chalk": "5.2.0",
		"clsx": "^2.1.1",
		"commander": "^10.0.0",
		"execa": "^7.0.0",
		"fs-extra": "^11.1.0",
		"node-fetch": "^3.3.0",
		"ora": "^6.1.2",
		"prompts": "^2.4.2",
		"tailwindcss-mosiui-mini": "^0.1.4",
		"tsconfig-paths": "^4.2.0",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.5",
		"@ianvs/prettier-plugin-sort-imports": "^3.7.2",
		"@types/estree": "^1.0.1",
		"@types/fs-extra": "^11.0.1",
		"@types/prettier": "^2.7.2",
		"@types/prompts": "^2.4.2",
		"acorn": "^8.8.2",
		"astring": "^1.8.5",
		"cross-env": "^7.0.3",
		"estree-walker": "^3.0.3",
		"magic-string": "^0.30.2",
		"prettier": "^2.8.8",
		"rimraf": "^4.1.3",
		"tsup": "^6.6.3",
		"type-fest": "^3.8.0",
		"typescript": "^4.9.3"
	}
}
